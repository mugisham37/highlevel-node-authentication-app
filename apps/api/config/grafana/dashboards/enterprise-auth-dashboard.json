{
  "dashboard": {
    "id": null,
    "title": "Enterprise Auth Backend Dashboard",
    "tags": ["enterprise", "authentication", "nodejs"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Authentication Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"enterprise-auth-backend\",path=~\"/auth.*\"}[5m])",
            "legendFormat": "Auth Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"enterprise-auth-backend\"}[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"enterprise-auth-backend\"}[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"enterprise-auth-backend\",status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"enterprise-auth-backend\"}[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Active Sessions",
        "type": "singlestat",
        "targets": [
          {
            "expr": "active_sessions_total{job=\"enterprise-auth-backend\"}",
            "legendFormat": "Active Sessions"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Database Connections",
        "type": "singlestat",
        "targets": [
          {
            "expr": "database_connections_active{job=\"enterprise-auth-backend\"}",
            "legendFormat": "DB Connections"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Redis Cache Hit Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "redis_cache_hits_total{job=\"enterprise-auth-backend\"} / (redis_cache_hits_total{job=\"enterprise-auth-backend\"} + redis_cache_misses_total{job=\"enterprise-auth-backend\"}) * 100",
            "legendFormat": "Cache Hit Rate %"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}